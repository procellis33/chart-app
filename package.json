{
  "name": "chart-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:js": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:scss": "stylelint \"src/**/*.scss\"",
